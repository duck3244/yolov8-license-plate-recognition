# .env.example
# 환경 변수 설정 예제 파일
# .env로 복사 후 사용하세요

# 모델 설정
LP_MODEL_PATH=yolov8n.pt
LP_CONFIDENCE_THRESHOLD=0.5

# Tesseract 설정 (Windows 예제)
# LP_TESSERACT_CMD=C:\Program Files\Tesseract-OCR\tesseract.exe

# 데이터베이스 설정
LP_DB_PATH=license_plates.db

# 웹 서버 설정
LP_WEB_HOST=0.0.0.0
LP_WEB_PORT=5000
LP_WEB_DEBUG=false

# 성능 설정
LP_MAX_WORKERS=4

# 보안 설정
LP_API_KEY=your_secret_api_key_here

# 로깅 설정
LP_LOG_LEVEL=INFO